# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# Database
ConnectionStrings__DefaultConnection=Server=(localdb)\MSSQLLocalDB;Database=DoSimple;Trusted_Connection=True;

# JWT Settings
Jwt__Key=YOUR_JWT_SECRET_KEY_MINIMUM_32_CHARACTERS_LONG
Jwt__Issuer=DoSimpleServer
Jwt__Audience=DoSimpleClient
Jwt__ExpiryInMinutes=15

# Email Settings
Email__SmtpHost=smtp.gmail.com
Email__SmtpPort=587
Email__SmtpUsername=your-email@gmail.com
Email__SmtpPassword=YOUR_SMTP_APP_PASSWORD_HERE
Email__FromEmail=noreply@dosimple.com
Email__FromName=DoSimple
Email__AppUrl=http://localhost:5248

# Admin Settings
AdminSettings__SecretKey=YOUR_ADMIN_SECRET_KEY_CHANGE_THIS
